    "url": "https://github.com/aronep6/password-builder.git"
  },
  "author": {
    "name": "aronep6",
    "url": "https://caze.ousmanedev.fr"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.4.7",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
