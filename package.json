  "description": "Lightweight password hashing tool using native node crypto module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "nodemon ./dist/playground.js",
    "tswatch": "tsc --watch --project ./tsconfig.dev.pckg.json",
